@model IEnumerable<MigrationTool.Models.ApplicationVersion>
<table class="table table-striped">
    <tr>
        <th>@Html.DisplayNameFor(model => model.Application.Name)</th>
        <th>@Html.DisplayNameFor(model => model.Name)</th>
        <th>@Html.DisplayNameFor(model => model.Notes)</th>
        <th>@Html.DisplayNameFor(model => model.TagsMetas)</th>
    </tr>
    @if (Model != null)
    {
        foreach (var item in Model.OrderBy(a => a.Application.Name).ThenBy(a => a.Name))
        {
            <tr>
                <td>@Html.ActionLink(item.Application.Name, "Details", "ApplicationVersion", new { id = item.Id }, null)</td>
                <td>@Html.DisplayFor(modelItem => item)</td>
                <td>@Html.DisplayFor(modelItem => item.Notes, "NoteCounts")</td>
                <td>@Html.DisplayFor(modelItem => item.TagsMetas, "TagList")</td>
            </tr>
        }
    }
</table>
@Html.Partial("_RecordCount", Model == null ? 0 : Model.Count())
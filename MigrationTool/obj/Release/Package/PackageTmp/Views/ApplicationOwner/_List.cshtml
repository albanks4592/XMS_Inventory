@model IEnumerable<MigrationTool.Models.ApplicationOwner>
<table class="table table-striped">
    <tr>
        <th>@Html.DisplayNameFor(model => model.FullName)</th>
        <th>@Html.DisplayNameFor(model => model.Division.Name)</th>
        <th>@Html.DisplayNameFor(model => model.TagsMetas)</th>
    </tr>
    @if (Model != null)
    {
        foreach (var item in Model.OrderBy(a => a.FirstName).ThenBy(a => a.LastName))
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item)</td>
                <td>@Html.DisplayFor(modelItem => item.Division.Name)</td>
                <td>@Html.DisplayFor(modelItem => item.TagsMetas, "TagList")</td>
            </tr>
        }
    }
</table>
@Html.Partial("_RecordCount", Model == null ? 0 : Model.Count())
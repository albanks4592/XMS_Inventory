@model IEnumerable<MigrationTool.Models.XenAppServer>
<table class="table table-striped">
    <tr>
        <th>@Html.DisplayNameFor(model => model.Name)</th>
        <th>@Html.DisplayNameFor(model => model.XenAppFarm.Name)</th>
        <th>@Html.DisplayNameFor(model => model.XenAppFarmFolder.Name)</th>
        <th>@Html.DisplayNameFor(model => model.OS)</th>
        <th>@Html.DisplayNameFor(model => model.Manufacturer)</th>
        <th>@Html.DisplayNameFor(model => model.Notes)</th>
        <th>@Html.DisplayNameFor(model => model.TagsMetas)</th>
    </tr>

    @foreach (var item in Model.OrderBy(x => x.Name)) {
        <tr>
            <td>@Html.DisplayFor(modelItem => item)</td>
            <td>@Html.DisplayFor(modelItem => item.XenAppFarm)</td>
            <td>@Html.DisplayFor(modelItem => item.XenAppFarmFolder)</td>
            <td>@Html.DisplayFor(modelItem => item.OS)</td>
            <td>@Html.DisplayFor(modelItem => item.Manufacturer)</td>
            <td>@Html.DisplayFor(modelItem => item.Notes, "NoteCounts")</td>
            <td>@Html.DisplayFor(modelItem => item.TagsMetas, "TagList")</td>
        </tr>
    }
</table>
@Html.Partial("_RecordCount", Model == null ? 0 : Model.Count())
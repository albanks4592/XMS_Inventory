@model IEnumerable<MigrationTool.Models.PublishedApplication>
<table class="table table-striped">
    <tr>
        <th>@Html.DisplayNameFor(model => model.Name)</th>
        <th>@Html.DisplayNameFor(model => model.XenAppFarm.Name)</th>
        <th>@Html.DisplayNameFor(model => model.XenAppFarmFolder)</th>
    </tr>
    @foreach (var item in Model.OrderBy(x => x.Name)) {
        <tr>
            <td>
                <div class="checkbox">
                    <label>
                        <input type="checkbox" name="selectedPublishedApplications" id="@item.Id" value="@item.Id" />
                        @if (item.Inactive)
                        {
                            <s>@Html.DisplayFor(modelItem => item.Name)</s>
                        }
                        else
                        {
                            @Html.DisplayFor(modelItem => item.Name)
                        }
                    </label>
                </div>
            </td>
            <td>
                <div class="checkbox">
                    @Html.DisplayFor(modelItem => item.XenAppFarm.Name)
                </div>
            </td>
            <td>
                <div class="checkbox">
                    @Html.DisplayFor(modelItem => item.XenAppFarmFolder)
                </div>
            </td>
        </tr>
    }
</table>
@Html.Partial("_RecordCount", Model == null ? 0 : Model.Count())
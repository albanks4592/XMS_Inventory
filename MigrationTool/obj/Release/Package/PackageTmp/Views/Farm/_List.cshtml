@model IEnumerable<MigrationTool.Models.XenAppFarm>
<table class="table table-striped">
    <tr>
        <th>@Html.DisplayNameFor(model => model.Name)</th>
        <th>@Html.DisplayNameFor(model => model.ToBeMigrated)</th>
        <th>@Html.DisplayNameFor(model => model.MigrationGroup.Name)</th>
        <th>@Html.DisplayNameFor(model => model.ReportForPxQ)</th>
        <th>@Html.DisplayNameFor(model => model.Notes)</th>
        <th>@Html.DisplayNameFor(model => model.TagsMetas)</th>
    </tr>
    @if (Model != null)
    {
        foreach (var item in Model.OrderBy(x => x.Name))
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item)</td>
                <td>@Html.DisplayFor(modelItem => item.ToBeMigrated)</td>
                <td>@Html.DisplayFor(modelItem => item.MigrationGroup)</td>
                <td>@Html.DisplayFor(modelItem => item.ReportForPxQ)</td>
                <td>@Html.DisplayFor(modelItem => item.Notes, "NoteCounts")</td>
                <td>@Html.DisplayFor(modelItem => item.TagsMetas, "TagList")</td>
            </tr>
        }
    }
</table>
@Html.Partial("_RecordCount", Model == null ? 0 : Model.Count())